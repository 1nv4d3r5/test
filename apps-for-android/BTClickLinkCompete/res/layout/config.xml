<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<Button android:id="@+id/start_hockey_server"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:text="Start Air Hockey Demo Server" />
	<Button android:id="@+id/start_hockey_client"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:text="Start Air Hockey Demo Client" />
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content"></TextView>

	<Button android:id="@+id/start_demo_server"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:text="Start Multiscreen Demo Server" />
	<Button android:id="@+id/start_demo_client"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:text="Start Multiscreen Demo Client" />
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content"></TextView>
    
  <Button android:id="@+id/start_video_server"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:text="Start Video Demo Server" />
  <Button android:id="@+id/start_video_client"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:text="Start Video Demo Client" />        
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content"></TextView>
    
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content"></TextView>	
	<Button android:id="@+id/goto_website"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:text="Visit the Click, Link, Compete Website" />
</LinearLayout>
